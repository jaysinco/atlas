{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Windows C++: Launch",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/bin/x86_64-debug/deep-samples",
            "args": [],
            "environment": [],
            "cwd": "${workspaceFolder}/bin/x86_64-debug",
            "stopAtEntry": false
        },
        {
            "name": "Linux C++: Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/bin/x86_64-debug/renju",
            "args": [
                "play",
                "0",
                "0",
                "10000"
            ],
            "environment": [],
            "cwd": "${workspaceFolder}/bin/x86_64-debug",
            "stopAtEntry": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "logging": {
                "moduleLoad": true,
                "trace": false
            }
        },
        {
            "name": "Linux CUDA: Launch",
            "type": "cuda-gdb",
            "request": "launch",
            "program": "${workspaceFolder}/bin/aarch64-debug/cuda-samples"
        },
        {
            "name": "Python: Launch",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/src/deep"
        },
        {
            "name": "Flutter: Launch",
            "type": "dart",
            "request": "launch",
            "program": "${workspaceFolder}/src/flapp/lib/main.dart"
        }
    ]
}