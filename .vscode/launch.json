{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Cpp",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/bin/debug/test",
            "args": [],
            "environment": [],
            "cwd": "${workspaceFolder}/bin/debug",
            "stopAtEntry": false,
            "logging": {
                "moduleLoad": true,
                "trace": false
            },
            "windows": {
                "type": "cppvsdbg",
            },
            "linux": {
                "MIMode": "gdb",
                "setupCommands": [
                    {
                        "description": "Enable pretty-printing for gdb",
                        "text": "-enable-pretty-printing",
                        "ignoreFailures": true
                    }
                ],
                "miDebuggerPath": "/usr/bin/gdb"
            },
        },
        {
            "name": "Flutter",
            "type": "dart",
            "request": "launch",
            "program": "src/hello_flutter/lib/main.dart"
        },
    ]
}